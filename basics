#include <iostream>
using namespace std;

// Base class
class Vehicle {
  public: 
    string brand = "bugatti";
    void color() {
      cout << â€œRed! \n" ;
    }
};

// Derived class
class Car: public Vehicle {
  public: 
    string model = "Mustang";
};
int main() {
  Car myCar;
  myCar.color();
  cout << myCar.brand + " " + myCar.model;
  return 0;
}

//OUTPUT:
//Red! 
//bugatti Mustang